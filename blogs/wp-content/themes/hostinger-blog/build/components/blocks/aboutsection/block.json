{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "theme-blocks/aboutsection",
  "title": "About section",
  "category": "theme-blocks",
  "icon": "smiley",
  "description": "About section",
  "attributes": {
    "title": {
      "type": "string"
    },
    "subtitle": {
      "type": "string"
    },
    "heroImgUrl": {
      "type": "string"
    },
    "heroImgId": {
      "type": "number"
    },
    "description": {
      "type": "string",
      "selector": "p"
    },
    "bottomImgLeftId": {
      "type": "number"
    },
    "bottomImgLeft": {
      "type": "string"
    },
    "bottomImgRightId": {
      "type": "number"
    },
    "bottomImgRight": {
      "type": "string"
    },
    "mediaURL": {
      "type": "string",
      "source": "attribute",
      "selector": "img",
      "attribute": "src"
    }
  },
  "supports": {
    "color": {
      "background": false,
      "text": false
    },
    "html": false
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}