{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "theme-blocks/newsletter-form",
  "title": "Newsletter form",
  "category": "theme-blocks",
  "icon": "admin-post",
  "description": "Newsletter form",
  "attributes": {
    "title": {
      "type": "string",
      "default": "Newsletter"
    },
    "description": {
      "type": "string",
      "default": "Subscribe to stay up-to-date with my latest creative projects, insights, and tips."
    },
    "privacyPolicy": {
      "type": "string"
    },
    "adminEmail": {
      "type": "string"
    }
  },
  "supports": {
    "color": {
      "background": false,
      "text": false
    },
    "html": false
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}